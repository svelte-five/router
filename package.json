{
  "name": "@svelte-five/router",
  "version": "0.1.0",
  "description": "Svelte 5 router with SSR support.",
  "repository": "https://github.com/svelte-five/router.git",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "lint": "eslint src",
    "format:check": "prettier src --check",
    "format": "prettier $(git diff --cached --name-only --diff-filter=ACMR src | sed 's| |\\\\ |g') --write --ignore-unknown",
    "test": "vitest",
    "build": "vite build",
    "prepare": "husky || true",
    "commit": "cz"
  },
  "files": [
    "dist/*",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.1",
    "@types/node": "^22.10.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.15.0",
    "esm-env": "^1.1.4",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "pkgroll": "^2.5.1",
    "prettier": "3.3.3",
    "regexparam": "^3.0.0",
    "semantic-release": "24.1.0",
    "svelte": "^5.2.8",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "esm-env": "^1.1.4",
    "regexparam": "^3.0.0",
    "svelte": "^5.2.0"
  }
}
